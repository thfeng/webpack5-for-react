{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "proseWrap": "never",
  "endOfLine": "lf",
  "overrides": [{
      "files": ".prettierrc",
      "options": {
        "parser": "flow"
      }
    },
    {
      "files": ["*.html", "*.ejs"],
      "options": {
        "parser": "html"
      }
    }
  ]
}
